{
    "name": "spress/spress",
    "description": "Static site generator with blogs support",
    "keywords": ["cms","blog","web","markdown","twig","html"],
    "license": "MIT",
    "homepage": "http://spress.yosymfony.com",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "twig/twig": ">=1.8,<2.0",
        "pimple/pimple": "~3.0",
        "symfony/console": "3.4.*",
        "symfony/finder": "3.4.*",
        "symfony/filesystem": "3.4.*",
        "symfony/yaml": "3.4.*",
        "symfony/event-dispatcher": "3.4.*",
        "michelf/php-markdown": "1.5.*",
        "dflydev/apache-mime-types": "1.0.*",
        "yosymfony/embedded-composer": "1.0.*",
        "yosymfony/config-loader": "1.3.*",
        "yosymfony/httpserver": "1.3.*",
        "yosymfony/resource-watcher": "1.2.*",
        "erusev/parsedown-extra": "0.7.*",
        "symfony/lts": "3.0"
    },
    "replace": {
        "spress/spress-core": "self.version"
    },
    "bin": [
        "bin/spress"
    ],
    "autoload": {
        "psr-4": {
            "Yosymfony\\Spress\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
